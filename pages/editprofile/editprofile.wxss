.editprofile-box {
  padding-top: 30rpx;
  color: #353535;
}
.editprofile-box .weui-cells .weui-cell .weui-label {
  font-size: 28rpx;
  color: #353535;
}
.editprofile-box .weui-cells .weui-cell .weui-input {
  font-size: 28rpx;
}
.editprofile-value {
  text-align: right;
  color: #999;
}
.editprofile-image-box {
  padding: 10rpx;
  overflow: hidden;
  height: 150rpx;
  box-sizing: border-box;
}
.editprofile-image {
  width: 130rpx;
  height: 130rpx;
  background-color: rgb(153, 148, 146);
  border-radius: 50%;
}
.editprofile-box .box-item{
  /* padding:10rpx 30rpx; */
  line-height: 80rpx;
}
.editprofile-box .button-edit{
  width:90%;
  margin:20rpx auto;
}


.regulate-dialog {
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, .5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  color: #353535;
}
.dialog-box {
  position: absolute;
  width: 100vw;
  height: 80vh;
  left: 0;
  bottom: -50vh;
  background-color: white;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  overflow: hidden;
}
.dialog-btn {
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  padding: 10rpx 20rpx;
  margin-bottom: 20rpx;
}
.dialog-title {
  transform: translateX(40rpx);
}
.dialog-close {
  width: 80rpx;
  height: 100%;
  display: flex;
  justify-content: flex-end;
}
.act-overlay-close {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 1rpx solid #999;
  font-size: 20rpx;
  text-align: center;
  line-height: 36rpx;
}
.act-overlay-close::after{
    content:'\e64d';
}
.dialog-ok {
  background-color: #ce4031;
  font-size: 28rpx;
  color: white;
  padding: 16rpx 0;
  text-align: center;
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 30rpx;
}
.dialog-image {
  height: 366rpx;
  width: 366rpx;
  background-color: orange;
  display: block;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.dialog-updata-image {
  color: #00d819;
  text-align: center;
  font-size: 28rpx;
}
.dialog-setname {
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.dialog-setname .weui-input {
  flex: 1;
}




.mycenter-arrow {
  font-size: 28rpx;
  color: #999;
}
.arrow::before {
  content: "\e6ba";
}