.userInfo-phone {
    background: red;
}
.userInfo-phone .act-overlay-main{
    height: 36%;
}

.userInfo-phone .editPhone-list{
    width:90%;
    overflow: hidden;
    margin:20rpx auto;
}
.userInfo-phone .editPhone-list .list-item,.userInfo-address .editAddress-list .list-items{
    display: flex;
    line-height: 80rpx;
    color:#353535;
    font-size: 28rpx;
    margin:0 auto;
    border-bottom: 1rpx solid #e5e5e5;
}

.userInfo-phone .list-item input,.editAddress-list .list-item input{
    flex: 1;
    height: 80rpx;
    line-height: 80rpx;
}
.userInfo-phone .list-item .item-getCode{
    min-width:200rpx;
    border-left: 1rpx solid #e5e5e5;
    color:#e94f4f;
    padding-left:10rpx;
    text-align: center;
}
.userInfo-phone .list-item .item-gray{
    color:#888;
}
.userInfo-phone .list-item .item-red{
    color:#e94f4f;
}
.userInfo-phone .overlay-sure,.userInfo-address .overlay-sure{
    width: 90%;
    margin:40rpx auto 20rpx auto;
    line-height: 80rpx;
    height: 80rpx;
}

