/* pages/userInfo/userInfo.wxss */
.mui-act-userInfo{
    width:100%;
    overflow: hidden;
   
}


.mui-act-userInfo .list-item{
    width:calc(100% - 20rpx);
    overflow: hidden;
    background: #fff;
    font-size: 32rpx;
    padding:20rpx 0;
    margin:20rpx auto;
    border-radius: 20rpx;
    text-indent: 20rpx;
}
.mui-act-userInfo .list-photo{
    padding-left:3%;
    padding-right:3%;
    margin-top:0;
    border-radius: 0;
    display: flex;
    font-size: 36rpx;
    border-top:1rpx solid #e5e5e5;
}
.mui-act-userInfo .list-photo .item-photo{
    width:120rpx;
    height:120rpx;
    border-radius: 120rpx;
}
.mui-act-userInfo .list-photo .item-name{
    line-height: 50rpx;
    /* margin-left:20rpx; */
}
.mui-act-userInfo .list-photo .main-nickName{
    margin-top:20rpx;
}
.mui-act-userInfo .list-photo .main-trust{
    font-size: 28rpx;
    color:#888;
}

.mui-act-userInfo .list-item .item-title{
    width:100%;
    line-height: 60rpx;
    border-bottom: 1rpx solid #e5e5e5;
    font-size: 32rpx;
    /* color:#e94f4f; */
    font-weight: bold;
}
.mui-act-userInfo .list-item .item-main{
    width:calc(100% - 80rpx);
    /* overflow: hidden; */
    display: flex;
    justify-content:space-between;
    text-indent: 0;
    padding:10rpx 40rpx 10rpx 0;
    margin:0 auto;
    /* margin-left:20rpx; */
    position: relative;
    min-height: 50rpx;
    line-height: 50rpx;
    border-bottom: 1rpx #e5e5e5 solid;
    font-size: 28rpx;

}
.mui-act-userInfo .list-item .item-main:last-child{
    border-bottom:none;
}
.mui-act-userInfo .list-item .item-main::after{
    content: '\e6ba';
    line-height: 60rpx;
    position: absolute;
    right:0rpx;
    top:50%;
    transform: translateY(-50%);
    color:#888;


}
.mui-act-userInfo .list-item .main-text{
    color:#888;
}
.mui-act-userInfo .list-item .main-auth{
    border-radius: 20rpx;
    border:1rpx solid #888;
    color:#888;
    padding:5rpx 20rpx;
    font-size: 24rpx;
    height:30rpx;
    line-height: 30rpx;
    margin-top:8rpx;
}
.mui-act-userInfo .list-item .auth-true{
    border-color:#00d819;
    color:#00d819;
}


